<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}pressing philippe auguste{% endblock %}</title>
        {% block stylesheets %}
            <link rel="icon" type="image/x-icon" href="{{ asset('icon.png') }}" />
            <link rel="stylesheet" type="text/css" href="{{ asset('build/css/app.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ asset('fontawesome/css/fontawesome-all.min.css') }}">
        {% endblock %}
    </head>
    <body>
    	<header>
    {% if is_granted('IS_AUTHENTICATED_FULLY') %}
        {% include 'navigationAdmin.html.twig' %}
    {% endif %}
        </header>
        {% block body %}{% endblock %}

        <footer>
    {% if is_granted('IS_AUTHENTICATED_FULLY') %}
            {% include 'footerAdmin.html.twig' %}
    {% endif %}
        </footer>
        {% block javascripts %}
        <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
        <a href="" class="pre-scrollable hidden-mobile visible">
            <i class="fa fa-chevron-up float-right"></i>
        </a>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
        
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
        <script type="text/javascript" src="{{ asset('build/js/app.js') }}"></script>
        {% endblock %}
    </body>
</html>